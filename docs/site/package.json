{
	"name": "site",
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"env": "bun run scripts/env.js",
		"dev": "bun run env && vite dev",
		"preview": "vite preview",
		"build": "bun run env && vite build",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint ."
	},
	"devDependencies": {
		"@eslint/compat": "^1.4.1",
		"@sveltejs/adapter-static": "^3.0.10",
		"@sveltejs/kit": "^2.50.0",
		"@sveltejs/vite-plugin-svelte": "^5.1.1",
		"@tailwindcss/typography": "^0.5.19",
		"@tailwindcss/vite": "^4.1.18",
		"beautify-qrcode": "^1.0.3",
		"clipboard": "^2.0.11",
		"eslint": "^9.39.2",
		"eslint-config-prettier": "^9.1.2",
		"eslint-plugin-svelte": "^2.46.1",
		"globals": "^15.15.0",
		"highlight.js": "^11.11.1",
		"highlightjs-svelte": "^1.0.6",
		"prettier": "^3.8.1",
		"prettier-plugin-svelte": "^3.4.1",
		"prettier-plugin-tailwindcss": "^0.6.14",
		"rollup-plugin-md-ts": "file:../../packages/rollup-plugin-md-ts",
		"rollup-plugin-stdf-icon": "file:../../packages/rollup-plugin-stdf-icon",
		"stdf": "file:../../packages/stdf",
		"svelte": "^5.47.1",
		"svelte-check": "^4.3.5",
		"svelte-confetti": "^2.3.2",
		"tailwindcss": "^4.1.18",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.53.1",
		"vite": "^6.4.1"
	}
}
